; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rougelike"
run/main_scene="res://hub/hub.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalConfig="*res://GlobalConfig.gd"
GameState="*res://game_state.gd"
SceneManager="*res://scene_manager.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=240
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/open_editors/plugin.cfg", "res://addons/sprite_painter/plugin.cfg")

[importer_defaults]

animation_library={
"animation/fps": 45.0
}

